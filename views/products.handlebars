                <!-- Success Transaction Message -->
                  <div class="row">
                    <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
                        <div id="success" class="alert alert-success" {{#if noMessages}}hidden{{/if}}>
                        {{successMsg}}   
                        </div>
                    </div>   
                  </div>
                  <!-- Success Message -->
                <div class="row">
                    {{#each products}}
                    <div class="col-md-4 text-left productItem" value = "{{this.id}}">
                        <a href="products/{{this.product}}">
                            <img style="max-height:200px" src="{{this.Images.0.url}}"/>

                        </a>
                        <br>
                        <div class="productName" value = "{{this.products}}"> 
                            <strong>{{this.product}}</strong>
                        </div>

                        <div class="productPrice" value = {{this.price}}>Price: ${{this.price}}</div>
                    </div>
